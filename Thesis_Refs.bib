eprint = {1509.05893},
file = {:C$\backslash$:/Users/liuzy/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Peng et al. - 2016 - Diffusion of ellipsoids in bacterial suspensions.pdf:pdf},
isbn = {9780874216561},
issn = {10797114},
journal = {Physical Review Letters},
mendeley-groups = {Active fluids/Enhanced diffusion,PhD-thesis-refs},
number = {6},
pages = {1--5},
