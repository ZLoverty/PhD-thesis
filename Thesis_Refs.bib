volume = {10},
year = {2016}
}

@article{Squires2010,
abstract = {In microrheology, the local and bulk mechanical properties of a complex fluid are extracted from the motion of probe particles embedded within it. In passive microrheology, particles are forced by thermal fluctuations and probe linear viscoelasticity, whereas active microrheology involves forcing probes externally and can be extended out of equilibrium to the nonlinear regime. Here we review the development, present state, and future directions of this field. We organize our review around the generalized Stokes-Einstein relation (GSER), which plays a central role in the interpretation of microrheology. By discussing the Stokes and Einstein components of the GSER individually, we identify the key assumptions that underpin each, and the consequences that occur when they are violated. We conclude with a discussion of two techniques-multiple particle-tracking and nonlinear microrheology-that have arisen to handle systems in which the GSER breaks down. Copyright {\textcopyright} 2010 by Annual Reviews. All rights reserved.},
author = {Squires, Todd M. and Mason, Thomas G.},
doi = {10.1146/annurev-fluid-121108-145608},
file = {:E$\backslash$:/Google Drive/Documents/Literatures/Active fluids/Enhanced diffusion/Squires2010.pdf:pdf},
issn = {00664189},
journal = {Annual Review of Fluid Mechanics},
keywords = {Fluctuation dissipation,Mobility,Rheology,Stokes-Einstein relation,Viscoelasticity},
mendeley-groups = {Active fluids/Enhanced diffusion},
pages = {413--438},
title = {{Fluid mechanics of microrheology}},
volume = {42},
year = {2010}
}

