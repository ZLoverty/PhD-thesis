isbn = {0027-8424},
issn = {00278424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biofluid dynamics,Flagella,Low reynolds number},
mendeley-groups = {Active fluids/Enhanced diffusion/experiment},
